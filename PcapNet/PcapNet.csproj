<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard1.6</TargetFramework>
    <AssemblyName>PcapNet</AssemblyName>
    <RootNamespace>PcapNet</RootNamespace>
    <Version>1.1</Version>
    <Description>A reader/writer for PCAP files.</Description>
    <Authors>Jeff Haynes</Authors>
    <Company />
    <PackageTags>Pcap</PackageTags>
    <PackageReleaseNotes>Updated serializer, switched to .net standard, switched interface to async, fixed timestamp bugs</PackageReleaseNotes>
    <PackageProjectUrl>https://github.com/jefffhaynes/PcapNgNet</PackageProjectUrl>
    <PackageLicenseUrl>https://opensource.org/licenses/mit-license.php</PackageLicenseUrl>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="BinarySerializer" Version="7.8.0" />
  </ItemGroup>

</Project>
